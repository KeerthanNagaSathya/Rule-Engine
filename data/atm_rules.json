{"rules":
[
  {
    "id":"rule_1",
    "name":"ATM_TTR",
    "description": "ATM TTR Identification",
    "is_valid": "true",
    "valid_from": "10/12/2021",
    "valid_till": "10/12/2999",
    "process": "identification",
    "key": "query_builder",
    "is_lookup": "false",
    "when":
    [
          {
            "field_name": "amount",
            "operator": ">=",
            "field_value": 10000,
            "join": "AND"
          },
          {
            "field_name": "cash_transaction",
            "operator": "=",
            "field_value": "true",
            "join": ""
          }
    ],
    "then":
        {
          "field_name": "is_ttr",
          "field_value": "true"
        }
  }
  ]
}